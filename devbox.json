{
    "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.14.2/.schema/devbox.schema.json",
    "packages": [
        "dpkg@latest",
        "go-task@latest",
        "fakeroot@latest",
        "git-lfs@latest"
    ],
    "shell": {
      "init_hook": [
        "echo 'Welcome to devbox!' > /dev/null",
        "source <(devbox completion $(basename $SHELL))",
        "source <(task --completion $(basename $SHELL))",
        "export PS1='ðŸ“¦ devbox:\\[\\033[01;34m\\]\\W\\[\\033[00m\\]> '",
        "git lfs install --skip-repo 2>/dev/null 1>/dev/null || true",
        "git lfs pull --all 2>/dev/null 1>/dev/null || true"
      ]
    }
  }
  