{
    "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.14.2/.schema/devbox.schema.json",
    "packages": [
        "dpkg@latest",
        "go-task@latest",
        "fakeroot@latest",
        "wget@latest",
        "gnutar@latest",
        "bzip2@latest"
    ],
    "shell": {
      "init_hook": [
        "echo 'Welcome to devbox!' > /dev/null",
        "source <(devbox completion $(basename $SHELL))",
        "source <(task --completion $(basename $SHELL))",
